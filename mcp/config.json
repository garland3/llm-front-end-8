{
  "external_mcps": [
    {
      "id": "rag_service",
      "name": "RAG Search",
      "description": "Document search and retrieval using RAG",
      "exclusive": false,
      "required_group": "mcp_users",
      "endpoint": "stdio://rag-mcp-server",
      "resources": [
        {
          "type": "template",
          "name": "search_query",
          "description": "Template for search queries",
          "schema": {
            "query": "string",
            "limit": "integer",
            "filter": "object"
          }
        }
      ]
    },
    {
      "id": "web_search",
      "name": "Web Search",
      "description": "Search the web for current information",
      "exclusive": true,
      "required_group": "admin",
      "endpoint": "stdio://web-search-mcp",
      "resources": [
        {
          "type": "template",
          "name": "web_query",
          "description": "Template for web search queries",
          "schema": {
            "query": "string",
            "max_results": "integer",
            "include_snippets": "boolean"
          }
        }
      ]
    }
  ]
}